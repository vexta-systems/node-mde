  "publishConfig": {
    "access": "public"
  },
  "author": "Lucas Melo <lucashpmelo94@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/@vexta-systems/node-mde/issues"
  },
  "homepage": "https://github.com/vexta-systems/node-mde#readme",
  "dependencies": {
    "axios": "^1.7.7",
    "fast-xml-parser": "^4.5.0",
    "luxon": "^3.5.0",
    "node-forge": "1.3.1",
    "xml-crypto": "^6.0.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.4",
    "@types/node-forge": "^1.3.11",
    "@types/xml-crypto": "^1.4.6",
